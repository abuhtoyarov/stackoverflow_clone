.votes
  .votes-error
  - if @votes.can_vote
    a href="#{@votes.resource_path}/voteup" data-method="patch" data-remote="true" data-type="json" rel="nofollow" Vote up
    a href="#{@votes.resource_path}/votedown" data-method="patch" data-remote="true" data-type="json" rel="nofollow" Vote down
  - else
    a href="#{@votes.resource_path}/unvote" data-method="patch" data-remote="true" data-type="json" rel="nofollow" Unvote
  .score
    p= @votes.score